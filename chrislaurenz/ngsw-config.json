{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "dataGroups": [{
      "name": "api-main",
      "urls": [
        "https://api.chrislaurenz.de/api/product"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 500,
        "maxAge": "1d"
      }
    },
    {
      "name": "api-types",
      "urls": [
        "https://api.chrislaurenz.de/api/product_category",
        "https://api.chrislaurenz.de/api/product_collection",
        "https://api.chrislaurenz.de/api/product_size",
        "https://api.chrislaurenz.de/api/product_type"
      ],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 300,
        "maxAge": "1d",
        "timeout": "10m"
      }
    }
  ],
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "navigationUrls": [
    "/**",
    "!/**/*.*",
    "!/**/*__*",
    "!/**/*__*/**"
  ]
}
